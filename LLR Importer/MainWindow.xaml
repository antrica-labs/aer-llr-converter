<Window x:Class="LLRImporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LLR Importer" SizeToContent="WidthAndHeight" >
    <StackPanel Margin="10">
        <Label>Select LLR file to convert (AER)</Label>
        <Button Click="Button_Click">Browse</Button>
        <ListView Name="FileContentViewer" IsEnabled="False" Height="300" Width="500" Margin="0,5" />
        <Button Name="SaveCSVButton" IsEnabled="False" Click="SaveCSVButton_Click">Save CSV</Button>
    </StackPanel>
</Window>
